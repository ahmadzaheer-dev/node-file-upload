<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>View files</title>
</head>

<body>
    <% if(files){ %>
    <% files.forEach(function(file) { %>
    <div>
        <img src="view/<%= file.filename %>" alt="">
    </div>
    <% }) %>
    <% } else { %>
    <p>No files to show</p>
    <% } %>
</body>

</html>